{
  "format_version": "1.10.0",
  "minecraft:attachable": {
    "description": {
      "identifier": "ldns:error_helmet.player",
      "item": { "ldns:error_helmet": "query.is_owner_identifier_any('minecraft:player')" },
      "materials": {
        "default": "armor",
        "enchanted": "armor_enchanted"
      },
      "textures": {
        "default": "textures/models/armor/error_1",
        "enchanted": "textures/misc/enchanted_item_glint"
      },
      "geometry": {
        "default": "geometry.player.armor.helmet"
      },
      "scripts": {
        "parent_setup": "variable.helmet_layer_visible = 0.0;",
        "animate": [
          "offset"
        ]
      },
      "animations": {
        "offset": "animation.armor.helmet.offset"
      },
      "render_controllers": [ "controller.render.armor" ]
    }
  }
}